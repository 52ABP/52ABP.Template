{
  "name": "yoyo-cms-template",
  "version": "4.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --port 4200 -o",
    "build": "ng build --prod --build-optimizer",
    "test": "ng test",
    "lint": "npm run lint:ts && npm run lint:style",
    "e2e": "ng e2e",
    "analyze": "ng build --prod --build-optimizer --stats-json",
    "test-coverage": "ng test --code-coverage --watch=false",
    "lint:ts": "tslint -p src/tsconfig.app.json -c tslint.json 'src/**/*.ts'",
    "lint:style": "stylelint \"{src}/**/*.less\" --syntax less",
    "lint-staged": "lint-staged",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "hmr": "ng serve --host 0.0.0.0 --port 4200 4201 --hmr -e=hmr"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/http": "^7.2.16",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@antv/data-set": "^0.10.2",
    "@antv/g2": "^3.5.11",
    "@antv/g2-plugin-slider": "^2.1.1",
    "@aspnet/signalr": "1.1.4",
    "@delon/abc": "8.8.0",
    "@delon/acl": "8.8.0",
    "@delon/cache": "8.8.0",
    "@delon/chart": "8.8.0",
    "@delon/form": "8.8.0",
    "@delon/mock": "8.8.0",
    "@delon/theme": "8.8.0",
    "@delon/util": "8.8.0",
    "abp-ng2-module": "^5.1.1",
    "abp-web-resources": "^5.1.1",
    "ajv": "^6.10.2",
    "core-js": "^3.6.3",
    "famfamfam-flags": "^1.0.0",
    "font-awesome": "^4.7.0",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.21",
    "ng-alain": "8.8.0",
    "ng-zorro-antd": "8.5.2",
    "ngx-countdown": "^8.0.3",
    "ngx-simplemde": "^1.1.1",
    "ngx-tinymce": "^7.0.0",
    "ngx-ueditor": "^2.1.3",
    "qrious": "^4.0.2",
    "rxjs": "^6.5.4",
    "rxjs-compat": "^6.5.4",
    "screenfull": "^5.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.22",
    "@angular/cli": "^8.3.22",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@angularclass/hmr": "^2.1.3",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.8",
    "@types/moment-timezone": "^0.5.9",
    "@types/node": "~13.1.6",
    "codelyzer": "~5.2.1",
    "editorconfig-tools": "^0.1.1",
    "husky": "^4.0.7",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~3.1.0",
    "karma-jasmine-html-reporter": "^1.5.1",
    "less-bundle-promise": "^1.0.7",
    "lint-staged": "^9.5.0",
    "ng-alain": "2.0.1",
    "nswag": "13.2.0",
    "prettier": "^1.19.1",
    "prettier-stylelint": "^0.4.2",
    "protractor": "~5.4.2",
    "stylelint": "^13.0.0",
    "stylelint-config-standard": "^19.0.0",
    "ts-node": "~8.6.1",
    "tslint": "~5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-language-service": "^0.9.9",
    "typescript": "3.4.5"
  },
  "lint-staged": {
    "*.{cmd,html,json,md,sh,txt,xml,yml}": [
      "editorconfig-tools fix",
      "git add"
    ],
    "*.ts": [
      "npm run lint:ts",
      "prettier --write",
      "git add"
    ],
    "*.less": [
      "npm run lint:style",
      "prettier --write",
      "git add"
    ],
    "ignore": [
      "src/assets/*"
    ]
  }
}
