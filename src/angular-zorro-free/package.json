{
  "name": "yoyo-cms-template",
  "version": "4.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --port 8080 -o",
    "build": "ng build --prod --build-optimizer",
    "test": "ng test",
    "lint": "npm run lint:ts && npm run lint:style",
    "e2e": "ng e2e",
    "analyze": "ng build --prod --build-optimizer --stats-json",
    "test-coverage": "ng test --code-coverage --watch=false",
    "lint:ts": "tslint -p src/tsconfig.app.json -c tslint.json 'src/**/*.ts'",
    "lint:style": "stylelint \"{src}/**/*.less\" --syntax less",
    "lint-staged": "lint-staged",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "hmr": "ng serve --host 0.0.0.0 --port 4200 4201 --hmr -e=hmr"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.10",
    "@angular/common": "^6.1.10",
    "@angular/compiler": "^6.1.10",
    "@angular/core": "^6.1.10",
    "@angular/forms": "^6.1.10",
    "@angular/http": "^6.1.10",
    "@angular/platform-browser": "^6.1.10",
    "@angular/platform-browser-dynamic": "^6.1.10",
    "@angular/router": "^6.1.10",
    "@antv/data-set": "^0.10.1",
    "@antv/g2": "^3.4.5",
    "@antv/g2-plugin-slider": "^2.1.1",
    "@aspnet/signalr": "1.0.3",
    "@delon/abc": "2.0.1",
    "@delon/acl": "2.0.1",
    "@delon/cache": "2.0.1",
    "@delon/form": "2.0.1",
    "@delon/mock": "2.0.1",
    "@delon/theme": "2.0.1",
    "@delon/util": "2.0.1",
    "@delon/chart": "2.0.1",
    "rxjs": "^6.2.1",
    "rxjs-compat": "^6.2.1",
    "ng-zorro-antd": "1.8.0",
    "ng-alain": "2.0.1",
    "abp-ng2-module": "^3.2.3",
    "abp-web-resources": "^3.8.4",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.21",
    "famfamfam-flags": "^1.0.0",
    "font-awesome": "^4.7.0",
    "core-js": "^2.5.4",
    "zone.js": "~0.8.26",
    "ajv": "^6.4.0",
    "ngx-countdown": "^3.1.0",
    "ngx-simplemde": "^1.1.1",
    "ngx-tinymce": "^2.1.1",
    "ngx-ueditor": "^2.1.3",
    "screenfull": "^3.3.1",
    "qrious": "^4.0.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.2",
    "@angular/cli": "^6.1.10",
    "@angular/compiler-cli": "^6.1.10",
    "@angular/language-service": "^6.1.10",
    "@angularclass/hmr": "^2.1.3",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "@types/moment-timezone": "^0.5.9",
    "codelyzer": "~4.3.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~2.9.2",
    "ng-alain": "2.0.1",
    "less-bundle-promise": "^1.0.7",
    "nswag": "11.20.1",
    "prettier": "^1.12.1",
    "prettier-stylelint": "^0.4.2",
    "stylelint": "^9.2.0",
    "stylelint-config-standard": "^18.2.0",
    "tslint-config-prettier": "^1.12.0",
    "tslint-language-service": "^0.9.9",
    "editorconfig-tools": "^0.1.1",
    "lint-staged": "^7.1.2",
    "husky": "^0.14.3"
  },
  "lint-staged": {
    "*.{cmd,html,json,md,sh,txt,xml,yml}": [
      "editorconfig-tools fix",
      "git add"
    ],
    "*.ts": [
      "npm run lint:ts",
      "prettier --write",
      "git add"
    ],
    "*.less": [
      "npm run lint:style",
      "prettier --write",
      "git add"
    ],
    "ignore": [
      "src/assets/*"
    ]
  }
}
